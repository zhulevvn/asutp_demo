(* Возвращает текущее системное время ПЛК, используя библиотеку SysLibTime *)
FUNCTION GetCurrentTimeDate : SystemTimeDate
VAR
	ctex : CurTimeEx;
	syt: SysTime64;
	cdt: SystemTimeDate;
END_VAR
(* @END_DECLARATION := '0' *)

syt.ulHigh := 0;
syt.ulLow := 0;

cdt.Year := 0;
cdt.Month := 0;
cdt.Day := 0;
cdt.DayOfWeek := 0;
cdt.Hour := 0;
cdt.Minute := 0;
cdt.Second := 0;
cdt.Milliseconds := 0;
cdt.dwHighMsec := 0;
cdt.dwLowMSecs := 0;

ctex(SystemTime := syt, TimeDate := cdt);

GetCurrentTimeDate := cdt;

END_FUNCTION


(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '4096' *)
PROGRAM PLC_PRG
VAR CONSTANT
	runTests : BOOL := FALSE; (* true - запуск тестов, false - рабочий режим *)
	beep_half_period : TIME := t#100ms; (* полупериод звукового сигнала, т.е. длительность звучания и паузы *)
END_VAR
VAR
	q : BOOL;
	firstPass : BOOL := TRUE;

	button AT %IX0.0.0.0 : BOOL; (* нормально разомкнутый контакт кнопки открывания двери - дискретный вход 1 *)
	lock AT %QD0.1.0 : REAL; (* нормально замкнутый контакт реле открывания замка - ВУ 1 *)

	lock_timeout AT %QD2.0.0 : DWORD; (* конф. параметр "время открытия замка, сек" *)

	el : eLock;
END_VAR

(* @END_DECLARATION := '0' *)

IF runTests THEN
	;
	tests();
ELSE
	IF firstPass THEN
		ClearScreen(0);
		SetWorkScreen(0);
	END_IF

	el(
		button := button, lock_timeout := DWORD_TO_TIME(lock_timeout * 1000), beep_half_period := beep_half_period, currentDateTime := GetCurrentTimeDate(),
		lock := lock, lines := displayLines
	);

	ShowString(0, 0, 0, displayLines[0]);
	ShowString(0, 0, 1, displayLines[1]);

	IF el.beep THEN
		BeepForTimeUser(time_ms := TIME_TO_INT(beep_half_period), freq := 500);
	END_IF

	firstPass := FALSE;
END_IF

END_PROGRAM

