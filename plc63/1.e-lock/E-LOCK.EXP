(* формирует строковое представление даты и времени в формате "дд.мм.гггг\nчч:мм:сс" для вывода на 2-х строчный дисплей *)
FUNCTION_BLOCK CurrentTimeDateBuilder
VAR_INPUT
	cdt: SystemTimeDate;
END_VAR
VAR_IN_OUT
	lines : DISPLAY_LINES;
END_VAR

(* @END_DECLARATION := '0' *)

lines[0] := '';

IF cdt.Day < 10 THEN
	lines[0] := CONCAT(lines[0], '0');
END_IF
lines[0] := CONCAT(lines[0], UINT_TO_STRING(cdt.Day));

lines[0] := CONCAT(lines[0], '.');

IF cdt.Month < 10 THEN
	lines[0] := CONCAT(lines[0], '0');
END_IF
lines[0] := CONCAT(lines[0], UINT_TO_STRING(cdt.Month));

lines[0] := CONCAT(lines[0], '.');

lines[0] := CONCAT(lines[0], UINT_TO_STRING(cdt.Year));

lines[1] := '';

IF cdt.Hour < 10 THEN
	lines[1] := CONCAT(lines[1], '0');
END_IF
lines[1] := CONCAT(lines[1], UINT_TO_STRING(cdt.Hour));

lines[1] := CONCAT(lines[1], ':');

IF cdt.Minute < 10 THEN
	lines[1] := CONCAT(lines[1], '0');
END_IF
lines[1] := CONCAT(lines[1], UINT_TO_STRING(cdt.Minute));

lines[1] := CONCAT(lines[1], ':');

IF cdt.Second < 10 THEN
	lines[1] := CONCAT(lines[1], '0');
END_IF
lines[1] := CONCAT(lines[1], UINT_TO_STRING(cdt.Second));

lines[0] := CONCAT(lines[0], '      ');
lines[1] := CONCAT(lines[1], '        ');

END_FUNCTION_BLOCK


(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION_BLOCK eLock
VAR_INPUT
	button : BOOL; (* нажата кнопка открывания двери *)
	lock_timeout : TIME; (* время открытия замка *)
	beep_half_period : TIME; (* полупериод звукового сигнала *)
	currentDateTime : SystemTimeDate; (* текущие дата и время для вывода на дисплей *)
END_VAR
VAR_IN_OUT
	lock : REAL; (* 1 - открыть замок, 0 - закрыть *)
	lines : DISPLAY_LINES; (* строки для вывода на дисплей *)
END_VAR
VAR_OUTPUT
	beep : BOOL; (* true - включить звуковой сигнал на время beep_half_period *)
END_VAR
VAR
	state : INT;
	lock_timer, beep_timer : TON;
	dtb : CurrentTimeDateBuilder;
END_VAR


(* @END_DECLARATION := '0' *)

CASE state OF
0:
	lock := 0;
	beep := FALSE;
	dtb(cdt := currentDateTime, lines := lines);

	IF button THEN
		state := 1;
	END_IF
1:
	lock := 1.0;
	lines[0] :=  'Дверь открыта   ';
	lines[1] := '                ';

	lock_timer(pt := lock_timeout, in := TRUE);
	beep_timer(pt := beep_half_period * 2, in := TRUE);

	IF lock_timer.Q THEN
		lock_timer(in := FALSE);
		beep_timer(in := FALSE);
		state := 0;
	ELSE
		IF beep_timer.Q THEN
			beep := TRUE;
			beep_timer(in := FALSE);
		ELSE
			beep := FALSE;
		END_IF;
	END_IF

END_CASE

END_FUNCTION_BLOCK


